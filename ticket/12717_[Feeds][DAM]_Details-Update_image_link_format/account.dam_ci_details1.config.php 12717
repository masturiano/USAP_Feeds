                                'function' => array(
                                    function($row){
                                        $shipping3 = $row['shipping3'];
                                        $worldpac = $row['worldpac'];
                                        if ($worldpac == '1') {
                                            $shipping = (($damPrice >= '50')) ? '0.00' : '10.00';
                                        } else {
                                            $shipping = $shipping3;
                                        }
                                        $shipping = $shipping + $row['handling3'];
                                        return $shipping;
                                    }
                                )
